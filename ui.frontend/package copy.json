{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "gulp clean && webpack --config ./webpack.prod.js && clientlib --verbose",
    "build": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -w ../ui.apps/src/main/content",
    "clean": "gulp clean"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@types/gulp": "^4.0.9",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.7.7",
    "aemsync": "^4.0.3",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.19.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.17",
    "del": "^6.0.0",
    "glob-import-loader": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.7",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "watch": "^1.0.2",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
