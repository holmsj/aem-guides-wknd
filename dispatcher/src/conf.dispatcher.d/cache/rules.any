#
# This file contains the cache rules, and can be customized.
#
# By default, it includes the default rules.
#

$include "./default_rules.any"

# Don't cache page with links based on login status
/00013 {
    /glob "/content/wknd/us/en/magazine.html"
    /type "deny"
}

/disable-nocache {
    /glob "*.nocache.html*"
    /type "deny"
}
